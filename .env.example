# Hugging Face access token (required for gated/private models).
HF_TOKEN=

# ColQwen3 embedding model (transformers / model card method)
MODEL_ID=TomoroAI/tomoro-ai-colqwen3-embed-4b-awq
COLQWEN_MAX_NUM_VISUAL_TOKENS=1280
COLQWEN_ATTN_IMPLEMENTATION=sdpa
COLQWEN_DTYPE=bfloat16
COLQWEN_PRELOAD_ON_START=1

# API ports
EMBED_PORT=8000
VLM_PORT=8001
QDRANT_HTTP_PORT=6333
QDRANT_GRPC_PORT=6334
GUI_PORT=8501

# llama.cpp server options (unsloth/Qwen3-VL-4B-Instruct-GGUF)
VLM_LLAMACPP_IMAGE=ghcr.io/ggml-org/llama.cpp:server-cuda
VLM_GGUF_REPO=unsloth/Qwen3-VL-4B-Instruct-GGUF
VLM_GGUF_FILE=Qwen3-VL-4B-Instruct-Q5_K_M.gguf
VLM_MMPROJ_FILE=mmproj-BF16.gguf
VLM_MODEL_ID=Qwen3-VL-4B-Instruct-Q5_K_M.gguf
VLM_CTX_SIZE=6144
LLAMA_N_GPU_LAYERS=99
LLAMA_THREADS=8
LLAMA_PARALLEL=1

# RAG GUI options
RAG_COLLECTION=jaxa_pdf_pages
RAG_TOP_K=2
RAG_DOCS_DIR=/app/docs
RAG_RENDER_SCALE=2.0
AUTO_INGEST_ON_START=1
